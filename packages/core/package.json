{
  "name": "@excilone/core",
  "version": "0.1.0",
  "description": "Simple, lightweight and versatile dependency injector.",
  "type": "module",
  "license": "Apache-2.0",
  "scripts": {
    "build": "tsdown",
    "types:check": "tsc -p tsconfig.lib.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./errors": {
      "import": {
        "types": "./dist/errors.d.mts",
        "default": "./dist/errors.mjs"
      },
      "require": {
        "types": "./dist/errors.d.cts",
        "default": "./dist/errors.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/excilone/excilone.git"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "keywords": [
    "dependency",
    "injector",
    "di",
    "ioc",
    "typescript",
    "javascript",
    "framework",
    "library"
  ],
  "author": "Cristobal Rodriguez <cristobal.rod0907@gmail.com>",
  "devDependencies": {
    "publint": "^0.3.16",
    "tsconfig": "workspace:*",
    "tsdown": "0.20.0-beta.1",
    "vitest": "*"
  }
}
